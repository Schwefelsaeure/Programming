   "source": [
    "def degree(radian_value):\n",
    "    return N((radian_value / pi) * 180)\n",
    "\n",
    "a = vector([2, 0, 0])\n",
    "b = vector([2, -1, 0])\n",
    "\n",
    "angle_in_radian = a.dot_product(b) / (a.norm() * b.norm())\n",
    "degree(arccos(angle_in_radian))"
   ]
  }
